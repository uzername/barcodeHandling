<html>
    <head>
        <meta charset="UTF8">
        <title> {{ localizedmessages['header']|default('SM_NM Scanner') }} </title>
         <style>

        </style>
        <link rel="stylesheet" type="text/css" href="../webtemplatesresources/commonbar.css" >
        <link rel="stylesheet" type="text/css" href="http://{{thishost}}/webtemplatesresources/datepicker/jquery.datepick.css"> 
        <script type="text/javascript" src="../webtemplatesresources/jquery-3.3.1.min.js">  </script>
        <script type="text/javascript" src="http://{{thishost}}/webtemplatesresources/datepicker/jquery.datepick.min.js"> </script>
        <script type="text/javascript" src="http://{{thishost}}/webtemplatesresources/datepicker/jquery.datepick-ru.min.js"></script>
        <script type="text/javascript"> 
        $('#startPicker,#endPicker').datepick({ onSelect: customRange, showTrigger: '<button type="button" class="trigger">...</button>'}); 
            function customRange(dates) { 
                if (this.id == 'startPicker') { 
                    $('#endPicker').datepick('option', 'minDate', dates[0] || null); 
                } 
                else { 
                    $('#startPicker').datepick('option', 'maxDate', dates[0] || null); 
                } 
            }
        </script>
    </head>
    <body>
        <div style="width:100%"> <a href="/">[<<<]</a> </div>
        <input type="text" id="startPicker" size="10" class="is-datepick"> 
        <input type="text" id="endPicker" size="10" class="is-datepick">
        <table class="stat-data">
            <thead> 
                <tr> 
                    <th class="stat-data" >&nbsp;</th> 
                    <th class="stat-data" > {{localizedmessages['knownbarcode_id_header']|default('known barcode id')}} </th> 
                    <th class="stat-data" > {{localizedmessages['rawbarcode_header']|default('sequence coded in barcode')}} </th> 
                    <th class="stat-data" > {{localizedmessages['scandatetime_header']|default('scanning date')}} </th> 
                </tr>
            </thead>
            {% for singlescan in scanlist %}
            <tr class="stat-data"> 
                <td class="stat-data">{{ singlescan.ID|default('-----')|e }}</td>
                <td class="stat-data">{{ singlescan.KNOWNBARCODE_ID|default('-----')|e }}</td>
                <td class="stat-data">{{ singlescan.RAWBARCODE|default('-----')|e }}</td>
                <td class="stat-data">{{ singlescan.SCANDATETIME|default('-----')|e }}</td>
            </tr>
            {% endfor %}
            
        </table>
    </body>
</html>